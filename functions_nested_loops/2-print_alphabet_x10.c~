#include <stdio.h>
#include <ctype.h>
#include "main.h"
/**
* print_alphabet_x10 - lettre minuscule
* Description: affiche les lettres minucules de la table ASCII
* Return:valeur 0
*/
void print_alphabet_x10(void)
{
int c = 97;
for (int i = 0; i < 10; i++)
{
while (c < 123)
{
_putchar(c);
c++;
}
}
_putchar('\n');
}
