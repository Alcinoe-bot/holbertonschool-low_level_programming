#include <stdio.h>
/**                                                                                                                                                                                                     
* main - affiche les petites lettres                                                                                                                                                                    
* Description: affiches les lettres minuscule de la table ASCII                                                                                                                                         
* Return:valeur 0                                                                                                                                                                                       
*/
int main(void)
{
for (int c = 97; c < 123; c++)
{
putchar(c);
}
return (0);
}
